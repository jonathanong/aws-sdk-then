{
  "name": "aws-sdk-then",
  "description": "Promisify the AWS SDK",
  "version": "1.0.0",
  "author": "Jonathan Ong <me@jongleberry.com> (http://jongleberry.com)",
  "license": "MIT",
  "repository": "jonathanong/aws-sdk-then",
  "dependencies": {
    "memorizer": "^1.0.1",
    "native-or-bluebird": "^1.2.0"
  },
  "devDependencies": {
    "aws-sdk": "^2.2.30",
    "babel-eslint": "4",
    "codecov": "1",
    "eslint": "1",
    "eslint-config-jongleberry": "1",
    "eslint-plugin-react": "3",
    "eslint-plugin-standard": "1",
    "ghooks": "1",
    "mocha": "2",
    "nyc": "5"
  },
  "scripts": {
    "lint": "eslint --cache --cache-location node_modules/.cache/eslint lib test",
    "test": "mocha --bail",
    "test-cov": "nyc --cache node_modules/mocha/bin/_mocha && nyc report --reporter=lcov"
  },
  "config": {
    "ghooks": {
      "pre-push": "npm run lint"
    }
  },
  "keywords": [
    "aws",
    "sdk",
    "then",
    "promise"
  ],
  "files": [
    "lib"
  ],
  "main": "lib"
}
